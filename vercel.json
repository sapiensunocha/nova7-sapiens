{
  "version": 2,
  "builds": [
    {
      "src": "nova7_backend/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "nova7_backend/app.py",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "nova7_backend/app.py"
    }
  ],
  "installCommand": "pip install -r nova7_backend/requirements.txt",
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}